{
	"version": 2,
	"buildCommand": "NITRO_PRESET=static COMPATIBILITY_DATE=2025-03-13 nuxt generate",
	"outputDirectory": ".output/public",
	"headers": [
		{
			"source": "/_nuxt/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			]
		}
	],
	"rewrites": [{ "source": "/(.*)", "destination": "/index.html" }]
}
