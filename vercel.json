{
	"version": 2,
	"builds": [
		{
			"src": "nuxt.config.ts",
			"use": "@nuxtjs/vercel-builder",
			"config": {
				"serverFiles": [".nuxt/dist/server/**/*", "server/**/*"]
			}
		}
	],
	"routes": [
		{
			"src": "/sw.js",
			"continue": true,
			"headers": {
				"Cache-Control": "public, max-age=0, must-revalidate",
				"Service-Worker-Allowed": "/"
			}
		}
	]
}
