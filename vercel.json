{
	"version": 2,
	"builds": [
		{
			"src": "package.json",
			"use": "@vercel/static-build",
			"config": {
				"zeroConfig": true,
				"framework": "nuxt"
			}
		}
	],
	"routes": [
		{
			"src": "/_nuxt/(.*)",
			"headers": {
				"Cache-Control": "public,max-age=31536000,immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "/(.*)",
			"dest": "/index.html"
		}
	],
	"buildCommand": "NITRO_PRESET=vercel NITRO_PRERENDER=false COMPATIBILITY_DATE=2025-03-13 nuxt build",
	"outputDirectory": ".output/public"
}
