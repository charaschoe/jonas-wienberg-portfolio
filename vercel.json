{
	"version": 2,
	"buildCommand": "chmod +x build.sh && ./build.sh",
	"outputDirectory": ".output/public",
	"routes": [
		{
			"src": "/_nuxt/(.*)",
			"headers": {
				"Cache-Control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{ "handle": "filesystem" },
		{ "src": "/(.*)", "dest": "/index.html" }
	]
}
